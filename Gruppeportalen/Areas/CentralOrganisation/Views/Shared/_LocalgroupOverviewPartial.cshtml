@model LocalGroup

<div class="card-header">
    <h3>@Model.GroupName</h3>
</div>
    <div class="card-details">
        <input type="hidden" asp-for="Id" />
            <p><strong>Adresse:</strong>@Model.Address</p>
            <p><strong>By:</strong>@Model.City</p>
            <p><strong>Fylke:</strong>@Model.County</p>
            <p><strong>Postnummer:</strong>@Model.Postcode</p>
    </div>
    <div>
        <a class="custom-orange-btn" asp-route-id="@Model.Id" asp-area="CentralOrganisation" asp-controller="Localgroup" asp-action="Edit" >Rediger</a>
        <a href="" class="custom-blue-btn">Slett</a>

    </div>

