@model Gruppeportalen.Areas.ViewModels.ApplicationPrivateUser

@{
Layout = "_Layout";
}
@Html.Partial("_PrivateUserSidebar")

<div class="main-content">
<h2>Rediger Brukerinfo</h2>

<form asp-action="Edit" method="post">
    <div class="form-group">
        <label for="Email">E-post</label>
        <input asp-for="Email" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Firstname">Fornavn</label>
        <input asp-for="Firstname" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Lastname">Etternavn</label>
        <input asp-for="Lastname" class="form-control" />
    </div>

    <div class="form-group">
        <label for="PhoneNumber">Telefonnummer</label>
        <input asp-for="Telephone" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Address">Adresse</label>
        <input asp-for="Address" class="form-control" />
    </div>

    <div class="form-group">
        <label for="City">By</label>
        <input asp-for="City" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Postcode">Postkode</label>
        <input asp-for="Postcode" class="form-control" />
    </div>

    <div class="form-group">
        <label for="DateOfBirth">FÃ¸dselsdato</label>
        <input asp-for="DateOfBirth" class="form-control" type="date" />
    </div>

    <button type="submit" class="btn btn-primary">Lagre</button>
    <a asp-action="Index" class="btn btn-secondary">Avbryt</a>
</form>
</div>