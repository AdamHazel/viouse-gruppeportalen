@model Gruppeportalen.Areas.PrivateUser.Models.ViewModels.CompleteLocalGroupOverview

@{
    ViewBag.Title = "Mine Lokallag";
    Layout = "_Layout";
}
<div class="find-localgroup-container">
<a class="find-localgroup-btn" asp-area="PrivateUser" asp-controller="Search" asp-action="Index">Finn nytt lokallag</a>
</div>
<div class="main-container">
    
    <div class="sidebar">
        @Html.Partial("_PrivateUserSidebar")
    </div>
    <div class="main-content">
        <partial name="_AdminLocalGroupsOverview" model="@Model.AdminOverview"/>
        <partial name="_MyPersonsLocalGroupsOverview" model="@Model.PersonOverview"/>
    </div>
</div>